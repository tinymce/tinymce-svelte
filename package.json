{
  "name": "@tinymce/tinymce-svelte",
  "version": "3.2.1-rc",
  "description": "TinyMCE Svelte Component",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/tinymce/tinymce-svelte.git"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "svelte": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/ts/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "lint": "eslint src/main/**/*.{ts,svelte}",
    "test": "echo 'No tests hooked up yet'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook build && gh-pages -d ./storybook-static -u 'tiny-bot <no-reply@tiny.cloud>' --nojekyll",
    "prepublishOnly": "yarn run build"
  },
  "keywords": [
    "tinymce",
    "svelte",
    "component"
  ],
  "author": "Tiny",
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt"
  ],
  "peerDependencies": {
    "tinymce": "^8.0.0 || ^7.0.0 || ^6.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "tinymce": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-svelte-csf": "^4.2.0",
    "@storybook/svelte": "^8.6.14",
    "@storybook/svelte-vite": "^8.6.14",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tinymce/beehive-flow": "^0.19.0",
    "@tinymce/eslint-plugin": "^2.4.0",
    "@tsconfig/svelte": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-loader": "^8.4.1",
    "eslint": "^8",
    "eslint-plugin-svelte": "^2.46.1",
    "gh-pages": "^6.3.0",
    "react": "^18",
    "react-dom": "^18",
    "rollup": "^2.79.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^8.6.14",
    "svelte": "^4.2.20",
    "svelte-check": "^3.8.6",
    "svelte-loader": "^3.2.4",
    "svelte-preprocess": "^5.1.4",
    "tinymce": "^8.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "webpack": "^5.102.1"
  },
  "resolutions": {
    "browserslist": "^4.16.5",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3"
  },
  "dependencies": {}
}
